<?xml version='1.0' encoding='utf-8'?>
<widget id="com.scotiabankhockeyclub.skaters" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>ScotiabankSkaters</name>
    <description>
        Scotiabank App for Skaters registration.
    </description>
    <author email="orlando.vicuna@majesticagency.ca" href="http://majesticagency.ca">
        Majestic Agency
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="http://scotia-hockey-hub.majesticdev.ca" />
    <access origin="https://scotiabankhockeyclub.com" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="FadeSplashScreenDuration" value="0" />
</widget>
